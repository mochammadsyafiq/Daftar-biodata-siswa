# 1. Buka terminal atau command prompt
# 2. Masuk ke folder repository yang sudah dibuat
cd path/ke/repository

# 3. Buat file baru dengan nama "biodata_siswa_2019.txt"
touch biodata_siswa_2019.txt

# 4. Buka file dan tambahkan daftar biodata siswa
echo "1. Nama   : Aco Wahyudi
   NIM    : H2020000001
   JK     : Laki - Laki
   Agama  : Islam
   TTL    : Ambon, 19 Agustus 2003
   Alamat : Jln. Loa Bakung RT.21 No.04

2. Nama   : Erna Mulyani
   NIM    : H2020000002
   JK     : Perempuan
   Agama  : Islam
   TTL    : Samarinda, 02 Juni 2003
   Alamat : Jln. Bantuas RT.01 No.18

3. Nama   : Indah Istiqomah
   NIM    : H2020000003
   JK     : Perempuan
   Agama  : Islam
   TTL    : Samarinda, 22 Desember 2002
   Alamat : Jln. Keledang RT.13 No.26

4. Nama   : Ilham ILyas
   NIM    : H2020000004
   JK     : Laki - Laki
   Agama  : Islam
   TTL    : Pinrang, 10 Maret 2003
   Alamat : Jln. Mangkupalas RT.07 No.12

5. Nama   : Rifqi Permana
   NIM    : H2020000005
   JK     : Laki - Laki
   Agama  : Islam
   TTL    : Ambon, 19 Agustus 2003
   Alamat : Jln. Loa Bakung RT.21 No.04
" > biodata_siswa_2019.txt

# 5. Tambahkan file ke staging area
git add biodata_siswa_2019.txt

# 6. Commit file dengan pesan
git commit -m "Menambahkan daftar biodata siswa angkatan 2019"

# 7. Push file ke repository GitHub (gantilah 'origin' dan 'main' jika diperlukan)
git push origin main
